{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "check": "npm run lint && npm run format && npm run stylelint",
    "dev": "nuxt dev",
    "fix": "npm run lint:fix && npm run format:fix && npm run stylelint:fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "project-tree": "node project-tree.js",
    "stylelint": "stylelint \"app/assets/css/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"app/assets/css/**/*.{css,scss}\" --fix"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/ui": "^3.3.3",
    "@tailwindcss/vite": "^4.1.12",
    "nuxt": "^4.1.0",
    "tailwindcss": "^4.1.12",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.92.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6"
  }
}
